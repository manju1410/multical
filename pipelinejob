node{
def gitbaseurl= "https://github.com/manju1410/multical"
def mvnHome= "/home/vijay/softwares/apache-maven-3.6.1-bin/apache-maven-3.6.1/bin/mvn"
stage('scm checkout'){
    git url:"${gitbaseurl}.git"
}
stage('compile-package'){
    sh "${mvnHome} package"
}
stage('SonarQube Analysis'){
    sh "${mvnHome} sonar:sonar"
}
stage('Deploy-to-Tomcat'){
ws("/home/vijay/mypipeline/workspace/deployjob/target"){
    sh "cp -rf *.war /home/vijay/softwares/apache-tomcat-8.5.35/webapps"
}
}
}
